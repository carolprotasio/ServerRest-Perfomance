log.info(&quot;Email: &quot; + email)
log.info(&quot;Senha: &quot; + senha)
log.info(&quot;Produto Nome: &quot; + produtoNome)
log.info(&quot;Produto Nome2: &quot; + produtoNome2)
log.info(&quot;Produto Preço: &quot; + produtoPreco)
