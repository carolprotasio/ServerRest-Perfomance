        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="JSR223 Sampler">
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="script">import com.github.javafaker.Faker

// Cria uma instância da biblioteca Faker
Faker faker = new Faker()

// Gera dados fictícios
String nome = faker.name().fullName()
String email = faker.internet().emailAddress()
String senha = faker.internet().password()
String produtoNome = faker.commerce().productName()
int produtoPreco = 120
String produtoDescricao = faker.lorem().sentence()
int produtoQtd = faker.number().numberBetween(1, 100) 

// Exibe os dados no log
log.info(&quot;Nome: &quot; + nome)
